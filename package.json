  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HomecareHomebase/azcli-npm/issues"
  },
  "homepage": "https://github.com/HomecareHomebase/azcli-npm#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.9",
    "@types/q": "^1.5.1",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "standard-version": "^4.4.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "q": "^1.5.1"
  }
